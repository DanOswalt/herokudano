<article id='project-template' type="text/x-handlebars-template">
  <div class='project'>
    <a href='{{html_url}}' target='_blank'><h1 class='project-name'>{{name}}</h1></a>
    <p class='project-language'>[{{language}}]</p>
    <p class='project-description'>{{description}}</p>
    <p class='project-updated'>Updated: {{daysAgo}}</p>
  </div>
</article>
